    				android:layout_width="fill_parent"
    				android:layout_height="fill_parent" >

					<ImageView android:id="@+id/ImageViewLogo"
					    android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_alignParentTop="true"
                        android:scaleType="fitCenter"
                        android:background="@drawable/gb_logo_big"
					    />
 	    
 	    
 	    
                          <Button
                              android:id="@+id/SendLinkButton"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                         	  android:layout_centerHorizontal="true"
                         	  android:textSize="36dip"
                         	  android:textColor="#0000ff"
                         	  android:layout_marginTop="30dip"
                         	  android:layout_below="@+id/ImageViewLogo"
                         	  android:layout_alignLeft="@+id/ImageViewLogo"
                         	  android:layout_alignRight="@+id/ImageViewLogo"
                              android:text="@string/btnsendlinktext"
                              android:background="@drawable/invite_button" />
                          <Button
                              android:id="@+id/ResumeButton"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                         	  android:layout_centerHorizontal="true"
                         	  android:textSize="36dip"
                         	  android:textColor="#0000ff"
                         	  android:layout_marginTop="30dip"
                         	  android:layout_below="@+id/SendLinkButton"
                         	  android:layout_alignLeft="@+id/ImageViewLogo"
                         	  android:layout_alignRight="@+id/ImageViewLogo"
                              android:text="@string/btnresumesessiontext"
                              android:background="@drawable/invite_button" />
						<Button
                              android:id="@+id/SettingsButton"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                         	  android:layout_centerHorizontal="true"
                         	  android:textSize="36dip"
                         	  android:textColor="#0000ff"
                         	  android:layout_marginTop="30dip"
                         	  android:layout_below="@+id/ResumeButton"
                         	  android:layout_alignLeft="@+id/ImageViewLogo"
                         	  android:layout_alignRight="@+id/ImageViewLogo"
                              android:text="@string/btnsettingstext"
                              android:background="@drawable/invite_button" />						<!-- <WebView 
							android:layout_width="fill_parent" 

    </RelativeLayout>
    
 	<FrameLayout android:id="@+id/frameLayoutRun"
    				android:layout_width="fill_parent"
    				android:layout_height="fill_parent"
    				android:gravity="bottom" >
        		<de.mjpegsample.MjpegView.MjpegView
							android:layout_width="fill_parent" 
							android:id="@+id/videoView" 
							android:layout_height="fill_parent"
							android:layout_marginTop="10dip"
							android:keepScreenOn="true"
							/>
 	    
 	    		<RelativeLayout android:id="@+id/RelativeLayoutButtons"
 	       			android:orientation="horizontal"
 	        		android:gravity="bottom"
 	        		android:layout_width="fill_parent"
 	        		android:layout_height="fill_parent">
 	                          <Button
                              android:id="@+id/StartButton"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_alignParentLeft="true"
                              android:textColor="#000000ff"
                              android:layout_alignParentBottom="true" 
                              android:background="@drawable/button_play"/>
 	                          <Button
                              android:id="@+id/PauseButton"
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:layout_alignParentLeft="true"
                              android:textColor="#000000ff"
                              android:layout_alignParentBottom="true" 
                              android:background="@drawable/button_pause"/>
 	                          <SurfaceView
                              android:id="@+id/CameraPreview"
                              android:layout_width="24dip"
                              android:layout_height="32dip"
							  android:layout_centerHorizontal="true"
							  android:layout_alignParentBottom="true"
							  android:visibility="visible" />
 	                          
 	                          <Button
 	                          	android:id="@+id/StopButton"
                              	android:layout_width="wrap_content"
                              	android:layout_height="wrap_content"
                 				android:layout_alignParentRight="true"
                 				android:layout_alignParentBottom="true"
                 				android:background="@drawable/button_stop"
                              	android:text="" />
 	                         <Button
 	                          	android:id="@+id/VolumeButton"
                              	android:layout_width="wrap_content"
                              	android:layout_height="wrap_content"
                 				android:layout_toLeftOf="@id/StopButton"
                 				android:layout_alignParentBottom="true"
                 				android:background="@drawable/button_volume"
                              	android:text="" /> 
 	    </RelativeLayout>
    </FrameLayout>
    <TextView
           android:id="@+id/StatusText"
           android:minLines="5"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:background="#000000ff"/>
</FrameLayout>